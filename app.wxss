.colltiparea {
  position: fixed;
  z-index: 100;
  animation: ani 3s infinite both;
  transform-origin: center bottom;
}

.colltiparea .colltip {
  position: relative;
  height: 60rpx;
  background: rgba(92, 235, 142,1);
  border-radius: 10rpx;
  text-align: center;
  font-size: 26rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  /* border:1rpx solid rgba(30,30,30,.1); */
  /* box-shadow: 0rpx 5rpx 20rpx -3rpx rgba(0, 0, 0, 0.15); */
}

.colltiparea .colltip .text {
  color: #ffffff;
  padding:0 20rpx;
  display: -webkit-box;
  overflow: hidden;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  max-width:300rpx;
}

.colltiparea .colltip .icon {
  margin-right:20rpx;
}

.colltiparea .colltip::after {
  content: '';
  width: 0;
  height: 0;
  position: absolute;
  top: -20rpx;
  right: 100rpx;
  border-style: solid;
  border-width: 10rpx 15rpx 15rpx 10rpx;
  border-color: transparent transparent rgba(92, 235, 142,1) transparent;
  font-size: 0;
}

@keyframes ani{
  0%,
  100%,
  20%,
  50%,
  80% {
  transform: translate3d(0,0,0); /*设置只在Z轴上移动*/
  }
  40%,
  43%{
  transform: translate3d(0,5px,0);
  }
  70%{
  transform: translate3d(0,3px,0);
  }
  90%{
  transform: translate3d(0,1px,0);
  }
}


