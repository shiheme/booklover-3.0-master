.tabwrapper .mainwrapper::after {
  content: '';
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 1);
  opacity: 0;
  z-index: 999;
  pointer-events: none;
  transition: .8s;
}

.tabwrapper.on .mainwrapper::after {
  opacity: .5;
  transition: .3s;
  pointer-events: auto;
}


